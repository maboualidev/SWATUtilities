<RunSingleSWATConfig>
	<RunID>null</RunID>
	<Inputs>
		<GroundWater>
			<FileNameFormat>%05d0001.gw</FileNameFormat>
			<Parameters>
				<Parameter>
					<Name>GW_REVAP</Name>
					<Value>0.2</Value>
					<AssignmentType>assign</AssignmentType>
					<ApplicableHRUs>10</ApplicableHRUs>
				</Parameter>
				<Parameter>
					<Name>REVAPMN</Name>
					<Value>1000</Value>
					<AssignmentType>assign</AssignmentType>
					<ApplicableHRUs>10,11</ApplicableHRUs>
				</Parameter>
			</Parameters>
		</GroundWater>
		<Soil>
			<FileNameFormat>%05d0001.sol</FileNameFormat>
			<Parameters>
				<Parameter>
					<Name>SOL_AWC</Name>
					<Value>0.2</Value>
					<AssignmentType>changeRate</AssignmentType>
					<ApplicableHRUs>10</ApplicableHRUs>
				</Parameter>
			</Parameters>
		</Soil>
	</Inputs>
	<SWATExecution>
		<WorkingDirectory>./tmpFolder</WorkingDirectory>
		<TemplateZIP>../SWATBase.zip</TemplateZIP>
		<DeleteWorkingDirectory>true</DeleteWorkingDirectory>
		<RollBack>false</RollBack>
	</SWATExecution>
	<Outputs>
		<File>
			<OriginalName>output.sub</OriginalName>
			<NewName>output_1.sub</NewName>
			<Location>../Outputs</Location>
		</File>
	</Outputs>
	<Display>
		<Verbose>true</Verbose>
		<VerboseLevel>1</VerboseLevel>
	</Display>
</RunSingleSWATConfig>
